@using SFA.DAS.Apim.Developer.Web.Infrastructure

@{
    ViewBag.Title = "Password changed";
    Layout = "_LayoutGeneric";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-xl">Password changed</h1>
        <p class="govuk-body">Your password has been successfully changed.</p>
        <a asp-route="@RouteNames.ThirdPartySignIn" role="button" draggable="false" class="govuk-button govuk-!-margin-top-6" data-module="govuk-button">Sign in</a>
    </div>
</div>
